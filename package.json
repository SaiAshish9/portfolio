{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.16.8",
    "axios": "^0.21.1",
    "codemirror": "^5.64.0",
    "js-beautify": "^1.14.0",
    "pdfmake": "^0.2.4",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-codemirror2": "^7.2.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-player": "^2.9.0",
    "react-router-dom": "^5.2.0",
    "react-router-i18n": "^1.1.0",
    "react-scripts": "4.0.3",
    "react-slick": "^0.28.1",
    "shuffle-array": "^1.0.1",
    "styled-components": "^5.3.0",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": "18.0.0"
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "extensions": [
            ".js",
            ".json",
            ".ts",
            ".tsx"
          ],
          "alias": {
            "underscore": "lodash"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "plugins": [
      "module-resolver"
    ],
    "settings": {
      "import/resolver": {
        "babel-module": {}
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "@babel/runtime": "^7.14.8",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.31.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-module-resolver": "^1.4.0"
  }
}
